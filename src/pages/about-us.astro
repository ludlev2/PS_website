                              r="15.91549431"></circle>
                            <circle
                              class="circle-chart-circle"
                              stroke-width="1"
                              stroke-dasharray="33,100"
                              stroke-linecap="round"
                              fill="none"
                              cx="16.91549431"
                              cy="16.91549431"
