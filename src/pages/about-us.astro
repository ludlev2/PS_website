                          <div class="position">Engineer</div>
                          <p class="description">
                            Google Software Engineer
                          </p>
                        </div>
                      </div>
                    </div>
                  </div>

                  <div class="column is-hidden-mobile"></div>
                </div>

                <!-- Spaced divider -->
                <div class="divider is-centered is-spacer"></div>
                <!-- Title & subtitle -->
                <h2 class="title is-light is-semibold has-text-centered is-spaced">
                  ...and the people backing us
                </h2>

                <!-- Content wrapper -->
                <div class="content-wrapper is-large">
                  <div class="columns is-vcentered is-multiline is-hcentered">
                    <!-- Advisor -->
                    <div class="column is-one-third">
                      <div class="advisor-container has-text-centered">
                        <!-- Picture -->
                        <div class="photo">
                          <img
                            src="/img/team/john.jpeg"
                            alt=""
                            data-aos="fade-down"
                            data-aos-delay="100"
                            data-aos-offset="200"
                            data-aos-easing="ease-out-quart"
                          />
                        </div>
                        <!-- Meta -->
                        <div class="info">
                          <div class="subtitle is-6 is-light is-compressed">
                            John Chang
                          </div>
                          <div class="position">
                          CEO of ECOVE inc. <br>
                          10+ years start up exp.
                          </br>
                          </div>
                        </div>
                      </div>
                    </div>

                    <!-- Advisor -->
                    <div class="column is-one-third">
                      <div class="advisor-container has-text-centered">
                        <div class="photo">
